<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="GamesTesterFrame">
		<title>Games Tester App For RemoteVariables</title>
		<size>870,562</size>
		<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="OurTextCtrl" member="yes">
			<maxlength>32</maxlength>
			<pos>600,448</pos>
			<size>256,24</size>
			<style>wxTE_PROCESS_ENTER</style>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
			<label>Label</label>
			<pos>784,416</pos>
		</object>
		<object class="wxButton" name="ID_BUTTON1" variable="SendButton" member="yes">
			<label>Send</label>
			<pos>752,480</pos>
			<size>104,27</size>
			<handler function="OnSendButtonClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxTextCtrl" name="ID_TEXTCTRL2" subclass="wxRichTextCtrl" variable="ChatTextCtrl" member="yes">
			<pos>600,18</pos>
			<size>256,424</size>
			<style>wxTE_MULTILINE</style>
		</object>
		<object class="wxStaticBox" name="ID_STATICBOX1" variable="StaticBox1" member="yes">
			<label>Game area</label>
			<pos>16,8</pos>
			<size>568,496</size>
		</object>
		<object class="wxButton" name="ID_BUTTON2" variable="NudgeButton" member="yes">
			<label>Nudge</label>
			<pos>600,488</pos>
			<size>56,19</size>
			<handler function="OnNudgeButtonClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="ClockTimer" member="yes">
			<interval>100</interval>
			<handler function="OnClockTimerTrigger" entry="EVT_TIMER" />
		</object>
	</object>
</wxsmith>
